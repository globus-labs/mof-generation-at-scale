&FORCE_EVAL
   &DFT
      BASIS_SET_FILE_NAME BASIS_MOLOPT
      POTENTIAL_FILE_NAME GTH_POTENTIALS

      CHARGE 0  ! Species should be charged balanced

      &QS
        METHOD GPW
      &END QS

      &SCF
         &MIXING
            METHOD BROYDEN_MIXING
            ALPHA 0.4
            NMIXING 16
         &END MIXING
      &END SCF
      &XC
         &XC_FUNCTIONAL PBE
         &END XC_FUNCTIONAL
        &VDW_POTENTIAL
            DISPERSION_FUNCTIONAL PAIR_POTENTIAL
            &PAIR_POTENTIAL
                TYPE DFTD3
                PARAMETER_FILE_NAME ./dftd3.dat
                REFERENCE_FUNCTIONAL PBE
            &END PAIR_POTENTIAL
        &END VDW_POTENTIAL
      &END XC
      &MGRID
         NGRIDS 5
         REL_CUTOFF [Ry] 60
      &END MGRID
   &END DFT
&END FORCE_EVAL
